  "name": "iot-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "7.1.1",
    "aws-sdk": "^2.92.0",
    "axios": "^0.16.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "6.23.0",
    "bootstrap": "4.0.0-alpha.6",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "chart.js": "^2.5.0",
    "classnames": "^2.2.5",
    "country-data": "^0.0.31",
    "css-loader": "0.28.4",
    "date-fns": "^1.29.0",
    "dotenv": "4.0.0",
    "enzyme": "^2.9.1",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "fontfaceobserver": "^2.0.13",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "jest-cli": "^21.0.2",
    "jwt-decode": "^2.2.0",
    "lodash-fp": "^0.10.4",
    "material-ui": "^0.18.7",
    "material-ui-chip-input": "^0.15.3",
    "normalize-scss": "^7.0.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "prettier": "^1.5.3",
    "prettier-check": "^1.0.0",
    "promise": "7.1.1",
    "react": "^15.6.2",
    "react-chartjs-2": "^2.6.4",
    "react-dev-utils": "^3.0.2",
    "react-dom": "^15.6.2",
    "react-error-overlay": "^1.0.9",
    "react-notification-system": "^0.2.16",
    "react-paginate": "^4.4.4",
    "react-places-autocomplete": "^5.4.2",
    "react-player-controls": "^0.5.16",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "next",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.6.1",
    "react-waypoint": "^7.1.0",
    "recordrtc": "^5.4.2",
    "redux": "^3.7.2",
    "redux-form": "^7.0.1",
    "redux-saga": "^0.15.5",
    "resize-observer-polyfill": "^1.4.2",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "style-loader": "0.18.2",
    "svg-sprite": "^1.3.7",
    "sw-precache-webpack-plugin": "0.11.3",
    "url-loader": "0.5.9",
    "uuid": "^3.1.0",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-json-bundler-plugin": "^1.0.0",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3",
    "ws": "^4.0.0"
  },
  "scripts": {
    "build:css": "node-sass-chokidar --include-path ./src/assets/styles --include-path ./node_modules src/ -o src/",
    "watch:css": "npm run build:css && node-sass-chokidar --include-path ./src/assets/styles --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start:js": "node scripts/start.js",
    "start": "yarn remove:css && npm-run-all -p watch:css start:js",
    "prettier:fix": "prettier --print-width 120 --trailing-comma es5 --single-quote --write \"src/**/*.js\"",
    "prettier": "prettier-check --print-width 120 --trailing-comma es5 --single-quote \"src/**/*.js\"",
    "lint:fix": "eslint --fix --ignore-path .gitignore -- src",
    "remove:css": "rimraf **/*.css",
    "build": "CI=false && yarn remove:css && npm run build:css && node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "devDependencies": {
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "svg-sprite-loader": "^3.0.10",
    "svgo-loader": "^1.2.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^containers(.*)$": "<rootDir>/src/containers$1",
      "^reducers(.*)$": "<rootDir>/src/reducers$1",
      "^sagas(.*)$": "<rootDir>/src/sagas$1",
      "^utils(.*)$": "<rootDir>/src/utils$1",
      "^constants(.*)$": "<rootDir>/src/constants$1",
      "^services(.*)$": "<rootDir>/src/services$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
